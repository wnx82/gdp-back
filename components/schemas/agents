components:
    schemas:
        agents:
            type: object
            properties:
                email:
                    type: string
                    description: L'email de l'agent
                password:
                    type: string
                    description: Le mot de passe de l'agent
                userAccess:
                    type: integer
                    description: Les droits d'accès de l'agent
                matricule:
                    type: string
                    description: Le matricule de l'agent
                firstname:
                    type: string
                    description: Le prénom de l'agent
                lastname:
                    type: string
                    description: Le nom de l'agent
                birthday:
                    type: string
                    format: date-time
                    description: La date de naissance de l'agent
                tel:
                    type: string
                    description: Le numéro de téléphone de l'agent
                adresse:
                    type: object
                    properties:
                        rue:
                            type: string
                            description: La rue de l'adresse de l'agent
                        numero:
                            type: string
                            description: Le numéro de l'adresse de l'agent
                picture:
                    type: string
                    description: La photo de l'agent
                formations:
                    type: array
                    description: Les formations suivies par l'agent
